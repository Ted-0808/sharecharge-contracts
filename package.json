{
  "name": "@motionwerk/sharecharge-contracts",
  "version": "0.0.6",
  "description": "Share & Charge smart contracts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "install": "mkdirp ~/.sharecharge/ && cp contract.defs.local.json ~/.sharecharge/",
    "lint": "solium -d contracts",
    "cover": "solidity-coverage",
    "test": "truffle test",
    "migrate": "truffle migrate",
    "geth-dev": "geth --dev --datadir=../.chaindata --dev.period 1 --rpc --rpcaddr 0.0.0.0 --rpccorsdomain=* --rpcapi=eth,personal,net,debug,shh --ws --wsaddr 0.0.0.0 --wsorigins=* --wsapi=eth,personal,net,pubsub,shh",
    "parity-dev": "parity --config network/dev.toml",
    "publish-development": "node bin/publish.js",
    "publish-test": "node bin/publish.js test",
    "publish-production": "node bin/publish.js production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motionwerkGmbH/sharecharge-contracts.git"
  },
  "author": "MotionWerk GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motionwerkGmbH/sharecharge-contracts/issues"
  },
  "homepage": "https://github.com/motionwerkGmbH/sharecharge-contracts#readme",
  "devDependencies": {
    "solidity-coverage": "^0.4.11",
    "solium": "^1.1.5",
    "truffle": "^4.1.7",
    "truffle-contract": "3.0.4",
    "web3": "^0.20.6",
    "zeppelin-solidity": "1.8.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
